	Doi = {https://doi.org/10.1016/j.jmarsys.2008.12.014},
	Issn = {0924-7963},
	Journal = {Journal of Marine Systems},
	Keywords = {Climate variability, Climate change, Ecosystem responses, Food webs, Mechanisms, Physiological effects},
	Note = {Impact of climate variability on marine ecosystems: A comparative approach},
	Number = {3},
	Pages = {374 - 388},
	Title = {On the processes linking climate to ecosystem changes},
	Url = {http://www.sciencedirect.com/science/article/pii/S092479630900092X},
	Volume = {79},
	Year = {2010},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S092479630900092X},
