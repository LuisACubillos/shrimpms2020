	Doi = {10.1002/jgrc.20337},
	Eprint = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1002/jgrc.20337},
	Journal = {Journal of Geophysical Research: Oceans},
	Keywords = {intrathermocline eddies, Peru-Chile upwelling system, equatorial subsurface water, coastal transition zone, Peru-Chile undercurrent, oxygen minimum zone},
	Number = {10},
	Pages = {4811-4821},
	Title = {Intrathermocline eddies in the coastal transition zone off central Chile (31--41$\,^{\circ}$S)},
	Url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1002/jgrc.20337},
	Volume = {118},
