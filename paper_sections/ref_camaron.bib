	Issn = {2211-4645},
	Journal = {Environmental Development},
	Keywords = {Recruitment, Spawning, Anchovy, Humboldt current system, Shared stock, Environment},
	Pages = {88 - 101},
	Title = {Sensitivity of recruitment rates anchovy (Engraulis ringens) to environmental changes in Southern Peru---Northern Chile},
	Url = {http://www.sciencedirect.com/science/article/pii/S2211464513000298},
	Volume = {7},
