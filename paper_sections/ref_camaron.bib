	Doi = {10.1175/JCLI3898.1},
	Eprint = {https://journals.ametsoc.org/jcli/article-pdf/19/20/5009/3803996/jcli3898\_1.pdf},
	Issn = {0894-8755},
	Journal = {Journal of Climate},
	Month = {10},
	Number = {20},
	Pages = {5009-5030},
	Title = {{Climate Variability, Fish, and Fisheries}},
	Url = {https://doi.org/10.1175/JCLI3898.1},
	Volume = {19},
