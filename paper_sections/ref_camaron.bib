	Journal = {Limnology and Oceanography},
	Number = {7},
	Pages = {1774-1780},
	Title = {Atlantic climate governs oceanographic and ecological variability in the Barents Sea},
	Url = {https://aslopubs.onlinelibrary.wiley.com/doi/abs/10.4319/lo.2001.46.7.1774},
	Volume = {46},
	Year = {2001},
	Bdsk-Url-1 = {https://aslopubs.onlinelibrary.wiley.com/doi/abs/10.4319/lo.2001.46.7.1774},
