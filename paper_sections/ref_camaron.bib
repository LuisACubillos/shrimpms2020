	Issn = {0718-560X},
	Journal = {Latin American Journal of Aquatic Research},
	Keywords = {pelagic fishery; environmental variability; conceptual model; northern Chile},
	Number = {2},
	Pages = {159--181},
	Title = {Coupling between the environment and the pelagic resources exploited off northern Chile: ecosystem indicators and a conceptual model},
	Url = {http://lajar.ucv.cl/index.php/rlajar/article/view/vol36-issue2-fulltext-3},
	Volume = {36},
	Year = {2008},
